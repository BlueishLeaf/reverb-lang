var fibonacci = fn(n) begin
    if n == 1 then
        return 0
    end
    else if n == 2 then
        return 1
    end
    return fibonacci(n - 1) + fibonacci(n - 2)
end

var forLoop = fn(start, stop, f) begin
    var loop = fn(i) begin
        if i < stop then
            print(f(i))
            loop(i + 1)
        end
    end
    loop(start)
end

forLoop(1, 11, fibonacci)
