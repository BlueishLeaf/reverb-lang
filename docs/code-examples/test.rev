var fibonacci = echo(n) begin
    if n == 1 then
        return 0
    end
    else if n == 2 then
        return 1
    end
    else then
        return fibonacci(n - 1) + fibonacci(n - 2)
    end
end

put(fibonacci(1))
put(fibonacci(2))
